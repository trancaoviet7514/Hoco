<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Hoco" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/main_dark</item>
        <item name="colorPrimaryVariant">@color/main_dark</item>
        <item name="colorOnPrimary">@color/main_dark</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/white</item>
        <item name="android:editTextColor">@color/black</item>
        <item name="android:textColorHint">#B8B6B6</item>
        <item name = "android:windowActionBar">false</item>
        <item name = "android:windowNoTitle">true</item>
        <item name="android:textColorSecondary">@color/white</item>
    </style>

</resources>